// C++ program to demonstrate array of strings using 
// 2D character array 
#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
	// Initialize array of pointer 
	const char *colour[4] = { "Sachin", "Dhoni", "Virat", "Rahul" }; 

	// Printing Strings stored in 2D array 
	for (int i = 0; i < 4; i++) 
		cout << colour[i] << "\n"; 

	return 0; 
} 
